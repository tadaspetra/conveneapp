# Read More Here https://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.dart]
indent_size = 4
continuation_indent_size = 8
combine_as_imports = true
max_line_length = 120
multi_line_output = 4
quote_type = single
